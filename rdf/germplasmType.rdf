<!-- ### Datatype declaration, Internal annotation properties ###
 Some internal annotation properties to provide the type for identifiers and alternative labels (skos:altLabel)
 There are different types of identifiers and declaring a datatype to keep them separate seems to be useful? 
 Examples how to declare datatypes for WIEWS and BCI identifiers:
  <dcterms:identifier rdf:datatype="&germplasmType;bioColID">http://biocol.org/urn:lsid:biocol.org:col:35293</dcterms:identifier>
  <dcterms:identifier rdf:datatype="&germplasmType;wiewsInstituteID">SWE054</dcterms:identifier>
 Example how to declare datatypes for alternative term labels:
  <skos:altLabel xml:lang="en" rdf:datatype="&germplasmType;mcpdTerm">DONORNUMB</skos:altLabel>
-->

  <rdfs:Datatype rdf:about="&germplasmType;wiewsInstituteID">
    <skos:prefLabel xml:lang="en">FAO WIEWS Institute Code</skos:prefLabel>
    <skos:definition xml:lang="en">A three letter country ISO code and a three (old) or four digit number.</skos:definition>
    <skos:example xml:lang="en">ITA247; ITA406; NOR051; SWE002; SWE054</skos:example>
    <skos:note xml:lang="en">The FAO WIEWS Institute Codes are available at http://apps3.fao.org/wiews/institute_query.htm</skos:note>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2014-01-16</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasmType;IdentifierType"/>
    <rdfs:isDefinedBy rdf:resource="&germplasmType;"/>
    <skos:inScheme rdf:resource="&germplasmType;"/>
    <vs:term_status>unstable</vs:term_status>
    <skos:relatedMatch rdf:resource="http://apps3.fao.org/wiews/wiews.jsp"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&germplasmType;bioColID">
    <skos:prefLabel xml:lang="en">Biodiversity Collections Index ID</skos:prefLabel>
    <skos:definition xml:lang="en">An index of biodiversity collections, providing persistent identifiers based on LSID URIs.</skos:definition>
    <skos:example xml:lang="en">http://biocol.org/urn:lsid:biocol.org:col:35293</skos:example>
    <skos:note xml:lang="en">The Biodiversity Collections Index is available at http://biocol.org/</skos:note>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2014-01-14</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasmType;IdentifierType"/>
    <rdfs:isDefinedBy rdf:resource="&germplasmType;"/>
    <skos:inScheme rdf:resource="&germplasmType;"/>
    <vs:term_status>unstable</vs:term_status>
    <skos:relatedMatch rdf:resource="http://biocol.org/"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&germplasmType;mcpdTerm">
    <skos:prefLabel xml:lang="en">Equivalent MCPD term</skos:prefLabel>
    <skos:definition xml:lang="en">Equivalent term from the FAO/Bioversity MultiCrop Passport Descriptor list.</skos:definition>
    <skos:example xml:lang="en">INSTCODE; ACCENUMB; COLLNUMB; etc.</skos:example>
    <skos:note xml:lang="en">The Multi-Crop Passport Descriptors (MCPD) was issued in a draft version in 1997 with the first official and stable version published in 2001. In 2012 a revision of the MCPD was published.</skos:note>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2014-01-16</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasmType;IdentifierType"/>
    <rdfs:isDefinedBy rdf:resource="&germplasmType;"/>
    <skos:inScheme rdf:resource="&germplasmType;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <dcterms:source rdf:resource="http://goo.gl/JI53g"/>
    <dcterms:bibliographicCitation>Alercia A., S. Diulgheroff, and T. Metz (2001). FAO/IPGRI Multi-crop passport descriptors, December 2001. International Plant Genetic Resources Institute (IPGRI) / Food and Agriculture Organization of the United Nations (FAO), Rome, Italy. Available at "http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf".</dcterms:bibliographicCitation>
    <dcterms:bibliographicCitation>Alercia A., S. Diulgheroff, and M. Mackay (2012). FAO/Bioversity Multi-crop passport descriptors v.2 [MCPD v.2]. Food and Agriculture Organization of the United Nations (FAO), and Bioversity International, Rome, Italy. 11 pp. Available at "http://www.bioversityinternational.org/index.php?id=244&amp;tx_news_pi1[news]=1350" and "http://www.bioversityinternational.org/uploads/tx_news/1526.pdf".</dcterms:bibliographicCitation>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&germplasmType;euriscoTerm">
    <skos:prefLabel xml:lang="en">Equivalent EURISCO term</skos:prefLabel>
    <skos:definition xml:lang="en">Equivalent term from the European Germplasm Search Catalog, amendment to the MCPD.</skos:definition>
    <skos:example xml:lang="en">NICODE; COLLDESCR; BREDDESCR; DONORDESCR; DUPLDESCR; ACCEURL</skos:example>
    <skos:note xml:lang="en">The EURISCO terms are available at "http://www.ecpgr.cgiar.org/fileadmin/www.ecpgr.cgiar.org/MISC/EURISCO_Descriptors.pdf"</skos:note>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2014-01-16</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasmType;IdentifierType"/>
    <rdfs:isDefinedBy rdf:resource="&germplasmType;"/>
    <skos:inScheme rdf:resource="&germplasmType;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:source rdf:resource="http://www.ecpgr.cgiar.org/fileadmin/www.ecpgr.cgiar.org/MISC/EURISCO_Descriptors.pdf"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&germplasmType;epgrisTerm">
    <skos:prefLabel xml:lang="en">Equivalent EPGRIS term</skos:prefLabel>
    <skos:definition xml:lang="en">Equivalent proposed term from the EPGRIS3 project team of the European Genebank Documentation group.</skos:definition>
    <skos:example xml:lang="en">DATASET; UPLOADERCODE; DATASET_REMARK; EXPERIMENT; EXPERIMENT_NUMBER; EXPERIMENT_DESCRIPTION; EXPERIMENT_YEAR; EXPERIMENT_LONGITUDE; EXPERIMENT_LATITUDE; EXPERIMENT_REPORT; TRAIT; TRAIT_NUMBER; TRAIT_NAME; TRAIT_REMARK; TRAIT_METHOD; GENOTYPE; GENOTYPE_NUMBER; GENOTYPE_NICODE; GENOTYPE_INSTCODE; GENOTYPE_ACCENUMB; GENOTYPE_GENUS; SCORE</skos:example>
    <skos:note xml:lang="en">The EPGRIS3 phenotype characterization and evaluation terms are available at "http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"</skos:note>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2014-01-16</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasmType;IdentifierType"/>
    <rdfs:isDefinedBy rdf:resource="&germplasmType;"/>
    <skos:inScheme rdf:resource="&germplasmType;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdfs:Datatype>



